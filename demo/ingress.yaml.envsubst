apiVersion: networking.k8s.io/v1
kind: Ingress 
metadata: 
  name: my-gateway-ingress
spec: 
  rules: 
    - host: $SESSION_NAME.$INGRESS_DOMAIN
      http: 
        paths:
          - path: /
            pathType: Prefix
            backend: 
              service:
                name: my-gateway 
                port:
                  number: 80 
